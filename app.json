{
  "expo": {
    "name": "cognitii-game",
    "slug": "cognitii-game",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/icon.png",
    "scheme": "cognitiigame",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "entryPoint": "./App.jsx",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "backgroundColor": "#ffffff"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-splash-screen"
    ]
  }
}